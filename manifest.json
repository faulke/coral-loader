{
  "name": "Coral Loader",
  "description": "Extend the functionality of SBNation Coral commenting system.",
  "version": "0.0.4",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "coral128.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": ["storage", "scripting", "webNavigation", "webRequest"],
  "action": {
    "default_popup": "popup.html"
  }
}
